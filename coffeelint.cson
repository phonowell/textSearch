#->前不得有空格
arrow_spacing:
    level: 'error'

#复杂度检测
cyclomatic_complexity:
    level: 'warn'

#单行最大字符数
max_line_length:
    level: 'warn'
    value: 255

#空函数
no_empty_functions:
    level: 'error'

#空参数
no_empty_param_list:
    level: 'error'

#运算符前后接空格
space_operators:
    level: 'error'

#逗号后接空格
spacing_after_comma:
    level: 'error'